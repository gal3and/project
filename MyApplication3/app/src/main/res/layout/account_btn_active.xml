<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/account_btn_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/white"
        android:clickable="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/exch"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/spinner"
                android:text="her" />

            <Spinner
                android:id="@+id/spinner"
                android:layout_width="150dp"
                android:layout_height="94dp"
                android:layout_alignBottom="@+id/end_button"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_toLeftOf="@+id/end_button"
                android:layout_toStartOf="@+id/end_button" />

            <TextView
                android:id="@+id/end_button"
                android:layout_width="56dp"
                android:layout_height="61dp"
                android:layout_alignParentTop="true"
                android:layout_toLeftOf="@+id/start_button"
                android:layout_toStartOf="@+id/start_button"
                android:background="@android:color/darker_gray"
                android:text="종료" />

            <TextView
                android:id="@+id/start_button"
                android:layout_width="56dp"
                android:layout_height="61dp"
                android:layout_alignBottom="@+id/end_button"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:background="@android:color/darker_gray"
                android:text="시작" />

            <EditText
                android:id="@+id/charge_text"
                android:layout_width="160dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/exch" />

            <EditText
                android:id="@+id/item_text"
                android:layout_width="100dp"
                android:layout_height="69dp"
                android:layout_alignBottom="@+id/charge_text"
                android:layout_toLeftOf="@+id/intput_button"
                android:layout_toStartOf="@+id/intput_button"
                android:background="@android:color/darker_gray" />

            <TextView
                android:id="@+id/intput_button"
                android:layout_width="71dp"
                android:layout_height="69dp"
                android:background="@android:color/darker_gray"
                android:text="입력"
                android:layout_above="@+id/Date_Text"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

            <ListView
                android:id="@+id/my_account"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/Date_Text" />

            <EditText
                android:id="@+id/Date_Text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/charge_text"
                android:ems="10"
                android:inputType="date" />

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="date"
                android:ems="10"
                android:id="@+id/editText"
                android:layout_above="@+id/Date_Text"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

        </RelativeLayout>
    </FrameLayout>


</LinearLayout>